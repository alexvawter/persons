            <listPerson>
                <person xml:id="person-2822">
                    <bibl xml:id="bib2822-1">
                        <title>Le martyrologe de Rabban Ṣalība</title>
                        <ptr target="http://syriaca.org/bibl/1396"/>
                        <citedRange unit="pp">129, 130, 162</citedRange>
                    </bibl>
                    <tei:persName xml:id="name2822-1" xml:lang="en" source=""
                        syriaca-tags="#syriaca-headword">
                        <tei:addName sort="3" type="">of Hah</tei:addName>
                        <tei:forename>Sliba</tei:forename>
                        <tei:addName sort="2" type="untagged-title">Rabban</tei:addName>
                    </tei:persName>
                    <tei:persName xml:id="namex-1" xml:lang="en-x-gedsh" source="bib2822-1"
                            ><tei:addName type="untagged-title" sort="3">of
                            Ḥaḥ</tei:addName><tei:forename>Ṣalība</tei:forename><tei:addName
                            type="untagged-title" sort="2">Rabban</tei:addName>Ṣalība</tei:persName>
                    <tei:persName xml:id="namex-1" xml:lang="fr" source="bib2822-1">Rabban Sliba de
                        Hah</tei:persName>
                    <tei:persName xml:id="namex-1" xml:lang="syr-Syrj" source="bib2822-1"
                        syriaca-tags="#syriaca-headword">ܪܒܢ ܨܠܝܒܐ ܚܐܚܝܐ</tei:persName>
                    <tei:idno type="URI">http://syriaca.org/person/2822</tei:idno>
                    <tei:note type="abstract">
                        <tei:quote source=""/>
                    </tei:note>
                    <tei:note type="abstract">Rabban Sliba (father of Rabban Joshua) was a monastic
                        author who composed a martyrology in the late thirteenth century or early
                        fourteenth century (some time after the death of <persName
                            ref="http://syriaca.org/person/239">Bar ʿEbroyo</persName>. One source
                        describes him as being from <placeName ref="http://syriaca.org/place/217"
                            >Ḥaḥ</placeName>.</tei:note>
                    <tei:floruit notAfter="1400" notBefore="" from="1286" source="bib2822-1"
                        >1286-1364</tei:floruit>
                    <tei:sex source="bib2822-1">Male</tei:sex>
                </person>
            </listPerson>
        </body>
